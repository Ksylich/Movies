{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "airbnb",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "import",
        "react"
    ],
    "parser": "babel-eslint",
    "rules": {
        "strict": 0,
        "indent": ["error", 2],
        "jsx-a11y/no-static-element-interactions": 0,
        "react/jsx-no-bind": [0],
        "react/prop-types": [1],
        "react/require-default-props": [0],
        "no-underscore-dangle": [2, { "allow": ["__REDUX_DEVTOOLS_EXTENSION_COMPOSE__"] }]
    }
  
}